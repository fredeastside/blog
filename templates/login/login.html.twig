{% extends 'two_column_layout.html.twig' %}

{% block content %}
    <h3>Войти</h3>
    {{ form_start(form) }}
    {% if error %}
        <div class="alert alert-danger">
            {{ error.messageKey|trans(error.messageData, 'security') }}
        </div>
    {% endif %}
    {{ form_widget(form) }}
    {{ form_end(form) }}
{% endblock %}